generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Product {
  id           Int    @id @default(autoincrement())
  name         String
  image        String
  priceInCents Int
  uuid         String @unique @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  description  String @default("")
  priceId      String
}
